@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
    var name = ViewData["MyName"];
    var age = ViewData["age"];
    List<QuickType.Specimen> allSpecimens = (List<QuickType.Specimen>)ViewData["allSpecimens"];
    var weatherMessage = ViewData["WeatherMessage"];

}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
    <p>My name @name</p>
    <p>My age @age</p>
    <h2>@weatherMessage</h2>
    <table>
        <tr>
            <th>Latitude</th>
            <th>Longitude</th>
            <th>Name</th>
            <th>Address</th>
            <th>Plant ID</th>
            <th>Specimen ID</th>
        </tr>
        @{
            @foreach (QuickType.Specimen specimen in allSpecimens)
            {
                <tr>
                    <td>@specimen.Lat</td>
                    <td>@specimen.Lng</td>
                    <td>@specimen.Genus @specimen.Species @specimen.Common</td>
                    <td>@specimen.Address</td>
                    <td>@specimen.PlantId</td>
                    <td>@specimen.SpecimenId</td>
                </tr>
            }
        }
    </table>

</div>
